{
  "name": "@fishjam-dev/js-server-sdk",
  "version": "0.1.0",
  "description": "Fishjam server SDK for JavaScript",
  "license": "Apache-2.0",
  "author": "Software Mansion (https://swmansion.com)",
  "repository": "github:fishjam-dev/js-server-sdk",
  "homepage": "https://github.com/fishjam-dev/js-server-sdk#readme",
  "bugs": "https://github.com/fishjam-dev/js-server-sdk/issues",
  "keywords": [
    "webrtc",
    "membrane",
    "fishjam"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./proto": "./dist/protos/fishjam/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs": "typedoc src src/protos/fishjam",
    "gen:proto": "sh codegen/protobuf.sh",
    "gen:api": "sh codegen/openapi.sh",
    "prepare": "tsc"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "protobufjs": "^7.2.6"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/node": "^20.12.7",
    "prettier": "3.2.5",
    "ts-proto": "^1.172.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}
