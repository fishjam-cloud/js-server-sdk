{
  "name": "@fishjam-cloud/fishjam-openapi",
  "version": "0.3.0",
  "private": true,
  "description": "Fishjam OpenAPI",
  "homepage": "https://github.com/fishjam-cloud/js-server-sdk",
  "author": "Fishjam Cloud Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishjam-cloud/js-server-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/fishjam-cloud/js-server-sdk/issues"
  },
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "codegen": "sh openapi.sh",
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "minify": false,
    "format": [
      "cjs",
      "esm"
    ],
    "outDir": "dist"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "tsup": "^8.3.5"
  }
}
