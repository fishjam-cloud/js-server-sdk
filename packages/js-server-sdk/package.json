{
  "name": "@fishjam-cloud/js-server-sdk",
  "version": "0.25.0",
  "description": "Fishjam server SDK for JavaScript",
  "homepage": "https://github.com/fishjam-cloud/js-server-sdk",
  "author": "Fishjam Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishjam-cloud/js-server-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/fishjam-cloud/js-server-sdk/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "webrtc",
    "fishjam",
    "server",
    "membrane"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./gemini": "./dist/integrations/gemini.js"
  },
  "scripts": {
    "build": "tsup --dts-resolve",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint . "
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/integrations/gemini.ts",
      "src/proto.ts"
    ],
    "noExternal": [
      "fishjam-openapi",
      "fishjam-proto"
    ],
    "minify": false,
    "format": [
      "cjs",
      "esm"
    ],
    "outDir": "dist"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "@google/genai": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@google/genai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@fishjam-cloud/fishjam-openapi": "workspace:*",
    "@fishjam-cloud/fishjam-proto": "workspace:*",
    "@openapitools/openapi-generator-cli": "^2.28.3",
    "@types/node": "^25.2.2",
    "@types/websocket": "^1.0.10",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typed-emitter": "^2.1.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,mjs,cjs}": [
      "eslint --fix --config packages/js-server-sdk/eslint.config.mjs"
    ]
  }
}
